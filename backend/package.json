{
  "name": "api",
  "version": "0.0.1",
  "description": "api based on NestJS, MariaDB, Redis",
  "author": "Event Panel",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/src/main",
    "start:inspect": "nest start --debug 0.0.0.0:9229 --watch --exec 'node --inspect-brk'",
    "prettier-check": "npx prettier --check .",
    "prettier-fix": "npx prettier --write .",
    "lint": "eslint \"{src,apps}/**/*.ts\" --fix",
    "lint:fix": "eslint --fix . --ext .ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "typeorm": "ts-node ./node_modules/typeorm/cli",
    "migration:run": "npm run typeorm migration:run -- -d ormconfig.ts",
    "migration:generate": "npm run typeorm -- -d ormconfig.ts migration:generate -n",
    "migration:create": "npm run typeorm migration:create -n",
    "migration:revert": "npm run typeorm -- -d ormconfig.ts migration:revert",
    "schema:drop": "npm run typeorm schema:drop -- -d ormconfig.ts",
    "setup:start": "yarn migration:run && yarn start:dev",
    "setup:dev": "yarn schema:drop && yarn migration:run && yarn start:dev",
    "setup:docker-stop": "docker-compose down"
  },
  "dependencies": {
    "@elastic/elasticsearch": "^8.15.0",
    "@eslint/migrate-config": "1.1.0",
    "@faker-js/faker": "^8.4.1",
    "@nestjs-modules/mailer": "1.8.1",
    "@nestjs/axios": "^3.0.2",
    "@nestjs/bull": "^10.1.1",
    "@nestjs/cache-manager": "^2.2.2",
    "@nestjs/common": "^9.0.0",
    "@nestjs/config": "^3.2.2",
    "@nestjs/core": "^9.0.0",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/mapped-types": "^2.0.5",
    "@nestjs/microservices": "^10.4.8",
    "@nestjs/passport": "^10.0.3",
    "@nestjs/platform-express": "^10.3.9",
    "@nestjs/swagger": "^7.3.1",
    "@nestjs/terminus": "^10.2.3",
    "@nestjs/typeorm": "^10.0.2",
    "@types/cache-manager": "^4.0.6",
    "@types/cache-manager-redis-store": "^2.0.4",
    "@types/compression": "^1.7.5",
    "@types/lodash": "^4.14.195",
    "apdex": "^1.0.0",
    "artillery-plugin-faker": "^5.0.2",
    "axios": "^1.7.2",
    "bull": "^4.14.0",
    "cache-manager": "^5.6.1",
    "cache-manager-redis-store": "2",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "compression": "^1.7.4",
    "express": "^4.19.2",
    "express-basic-auth": "^1.2.1",
    "express-rate-limit": "^7.3.1",
    "helmet": "^7.1.0",
    "lodash": "^4.17.21",
    "mariadb": "^3.3.1",
    "nestjs-pino": "^4.1.0",
    "node-cache-manager": "^1.2.0",
    "nodemailer": "^6.9.14",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "pino-elasticsearch": "^8.0.0",
    "pino-http": "^10.1.0",
    "pino-pretty": "^11.2.1",
    "pug": "^3.0.2",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.2.0",
    "swagger-ui-express": "^5.0.1",
    "ts-mockito": "^2.6.1",
    "typed-jest-expect": "^1.0.1",
    "typeorm": "^0.3.20",
    "typeorm-naming-strategies": "^4.1.0",
    "typeorm-transactional": "^0.5.0",
    "uuid": "^10.0.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^9.0.0",
    "@nestjs/schematics": "^9.0.0",
    "@nestjs/testing": "^9.0.0",
    "@types/eslint__js": "^8.42.3",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.14.9",
    "@types/nodemailer": "^6.4.15",
    "@types/passport-jwt": "^4.0.1",
    "@types/supertest": "^2.0.11",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin-tslint": "^7.0.2",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "eslint-plugin-sonarjs": "^0.19.0",
    "eslint-plugin-unicorn": "^46.0.0",
    "globals": "^15.6.0",
    "husky": "^9.0.11",
    "jest": "^29.7.0",
    "jest-environment-node": "^29.7.0",
    "jsonwebtoken": "^9.0.1",
    "mysql": "^2.18.1",
    "prettier": "^3.3.2",
    "source-map-support": "^0.5.20",
    "supertest": "^7.0.0",
    "ts-jest": "^29.1.5",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "4.1.1",
    "tslint": "^6.1.3",
    "typescript": "^5.5.2",
    "typescript-eslint": "^7.14.1"
  },
  "jest": {
    "modulePathIgnorePatterns": [
      "../../src/migrations",
      "../../src/modules/*/*/dto"
    ],
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "roots": [
      "<rootDir>/test/unit"
    ],
    "testMatch": [
      "**/?(*.)+(spec).ts"
    ],
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
